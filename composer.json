{
    "name": "phalcon/skeleton-micro",
    "description": "Skeleton Application for Phalcon Micro Framework",
    "keywords": ["framework", "phalcon", "micro"],
    "type": "project",
    "require": {
        "php": ">=5.3",
        "ext-phalcon": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "tests/unit"
        }
    }
}
